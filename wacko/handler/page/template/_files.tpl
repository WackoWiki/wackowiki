[ === FilePanel === ]
	[= s _ =
		<section id="section-files">
			<header id="header-files">
				<h1><a href="[ ' href ' ]" title="[ ' title | e attr ' ]">[ ' text ' ]</a></h1>
			</header>
			[= f _ =
				<div class="files">
					[ ' files ' ]
				</div>
			=]
			[= u _ =
				<div class="filesform">
					[ ' upload ' ]
				</div>
			=]
		</section>
	=]